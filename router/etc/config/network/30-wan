config device
	option name 'vlan-wan'
	option type '8021q'
	option ifname 'eth-wan'
	option vid '7'
	option ipv6 '0'

config interface 'wan'
	option device 'vlan-wan'
#	option proto 'dhcp'
	option proto 'pppoe'
	option username '{{ .params.pppoe.username }}'
	option password '{{ .params.pppoe.password }}'
